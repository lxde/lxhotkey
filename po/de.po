#: ../src/lxhotkey.c:244
#, c-format
msgid "Usage: %s global [<key>]         - show keys bound to action(s)\n"
msgstr "Aufruf: %s global [<taste>]         - Aktion für die Taste anzeigen\n"

#: ../src/lxhotkey.c:245
#, c-format
msgid "       %s global <action> <key>  - bind a key to the action\n"
msgstr "        %s global <aktion> <taste>  - Taste für die Aktion festlegen\n"

#: ../src/lxhotkey.c:246
#, c-format
msgid "       %s app [<key>]            - show keys bound to exec line\n"
msgstr "        %s app [<taste>]            - Befehl für die Taste anzeigen\n"

#: ../src/lxhotkey.c:247
#, c-format
msgid "       %s app <exec> <key>       - bind a key to some exec line\n"
msgstr "        %s app <befehl> <taste>     - Taste für den Befehl festlegen\n"

#: ../src/lxhotkey.c:248
#, c-format
msgid "       %s app <exec> --          - unbind all keys from exec line\n"
msgstr ""
"        %s app <befehl> --          - Tastenzuordnung zum Befehl löschen\n"

#: ../src/lxhotkey.c:250
#, c-format
msgid "       %s --gui=<type>           - start with GUI\n"
msgstr "        %s --gui=<typ>              - mit GUI starten\n"


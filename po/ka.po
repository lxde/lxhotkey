#: ../src/lxhotkey.c:244
#, c-format
msgid "Usage: %s global [<key>]         - show keys bound to action(s)\n"
msgstr ""
"Usage: %s global [<key>]         - ქმედებაზე მიბმული ღილაკების ჩვენება\n"

#: ../src/lxhotkey.c:245
#, c-format
msgid "       %s global <action> <key>  - bind a key to the action\n"
msgstr "       %s global <action> <key>  - ღილაკის მიბმა ქმედებაზე\n"

#: ../src/lxhotkey.c:246
#, c-format
msgid "       %s app [<key>]            - show keys bound to exec line\n"
msgstr ""
"       %s app [<key>]            - ბრძანებაზე მიბმული ღილაკების ჩვენება\n"

#: ../src/lxhotkey.c:247
#, c-format
msgid "       %s app <exec> <key>       - bind a key to some exec line\n"
msgstr "       %s app <exec> <key>       - ბრძანებისთვის ღილაკის მინიჭება\n"

#: ../src/lxhotkey.c:248
#, c-format
msgid "       %s app <exec> --          - unbind all keys from exec line\n"
msgstr ""
"       %s app <exec> --          - ბრძანებისთვის ყველა ღილაკის მხოსნა\n"

#: ../src/lxhotkey.c:250
#, c-format
msgid "       %s --gui=<type>           - start with GUI\n"
msgstr "       %s --gui=<type>           - გაშვება გრაფიკული ინტერფეისით\n"

#: ../src/lxhotkey.c:345
msgid "empty option name."

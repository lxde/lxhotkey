#: ../src/lxhotkey.c:244
msgid "Usage: %s global [<key>]         - show keys bound to action(s)\n"
msgstr ""
"Upotreba:\n"
" %s global [<taster>]      — prikazuje tastere pridružene radǌama(i)\n"

#: ../src/lxhotkey.c:245
#, c-format
msgid "       %s global <action> <key>  - bind a key to the action\n"
msgstr "       %s global <radǌa> <taster>  — pridružuje taster radǌi\n"

#: ../src/lxhotkey.c:246
msgid "       %s app [<key>]            - show keys bound to exec line\n"
msgstr ""
"       %s app [<taster>]            — prikazuje tastere pridružene liniji "
"naredbe\n"

#: ../src/lxhotkey.c:247
#, c-format
msgid "       %s app <exec> <key>       - bind a key to some exec line\n"
msgstr ""
"       %s app <naredba> <taster>       — pridružuje taster liniji naredbe\n"

#: ../src/lxhotkey.c:248
#, c-format
msgid "       %s app <exec> --          - unbind all keys from exec line\n"
msgstr ""
"       %s app <naredba> --          — razdružuje taster od linije naredbe\n"

#: ../src/lxhotkey.c:250
#, c-format
msgid "       %s --gui=<type>           - start with GUI\n"
msgstr ""
"       %s --gui=<vrsta>           — pokreće program kroz graf. sučeǉe\n"

